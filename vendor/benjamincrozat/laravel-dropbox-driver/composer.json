{
    "name": "benjamincrozat/laravel-dropbox-driver",
    "description": "Dropbox driver for Laravel.",
    "homepage": "https://github.com/benjamincrozat/laravel-dropbox-driver",
    "license": "WTFPL",
    "authors": [
        {
            "name": "Benjamin Crozat",
            "email": "benjamin@croz.at"
        }
    ],
    "support": {
        "issues": "https://github.com/benjamincrozat/laravel-dropbox-driver/issues"
    },
    "keywords": [
        "laravel",
        "driver",
        "dropbox"
    ],
    "autoload": {
        "psr-4": {
            "BC\\Laravel\\DropboxDriver\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ]
    },
    "require": {
        "php": ">=7.1",
        "spatie/flysystem-dropbox": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*|6.*|7.*",
        "orchestra/testbench": "3.*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "BC\\Laravel\\DropboxDriver\\ServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
